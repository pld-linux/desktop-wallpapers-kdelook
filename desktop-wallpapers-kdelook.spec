%install
rm -rf $RPM_BUILD_ROOT
install -d $RPM_BUILD_ROOT%{_datadir}/wallpapers
install *.{jpg,png} $RPM_BUILD_ROOT%{_datadir}/wallpapers

cd $RPM_BUILD_ROOT%{_datadir}/wallpapers
tar xzf %{SOURCE5}
tar xzf %{SOURCE14}
tar xf %{SOURCE21}

%clean
rm -rf $RPM_BUILD_ROOT

